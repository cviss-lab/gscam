<launch>

  <node name="gscam" pkg="gscam" type="gscam" output="screen">
    <param name="camera_info_url" value="file:///home/raza/hires_intrinsics/starling/1k/ost.yaml" />
    <param name="gscam_config" value="rtspsrc location=rtsp://192.168.0.2:8900/live latency=50 ! rtpjitterbuffer ! rtph264depay ! h264parse ! avdec_h264 ! queue max-size-buffers=0 leaky=downstream ! videoconvert" />
    <param name="frame_id" value="hires" />
    <param name="camera_name" value="hires" />
